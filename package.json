{
  "name": "vue-snap",
  "version": "1.1.0",
  "type": "module",
  "description": "Lightweight Carousel based on CSS Scroll Snap",
  "main": "dist/vue-snap.umd.js",
  "module": "dist/vue-snap.es.js",
  "unpkg": "dist/vue-snap.js",
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": {
      "import": "./dist/vue-snap.es.js",
      "require": "./dist/vue-snap.umd.js"
    },
    "./dist/vue-snap.css": "./dist/vue-snap.css"
  },
  "scripts": {
    "release": "vue-tsc -b && vite build -c ./config/vite.config.ts",
    "lint": "eslint 'src/**/*.{ts,vue}'",
    "test": "test --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bartdominiak/vue-snap.git"
  },
  "keywords": [
    "vue",
    "vue-snap",
    "carousel",
    "slider",
    "scroll-snap"
  ],
  "author": "smugers@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bartdominiak/vue-snap/issues"
  },
  "homepage": "https://github.com/bartdominiak/vue-snap#README",
  "peerDependencies": {
    "vue": "^3.5.16"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "lodash": "^4.17.21",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.10"
  }
}

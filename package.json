{
  "name": "vue-snap",
  "version": "1.0.1",
  "type": "module",
  "description": "Lightweight Carousel based on CSS Scroll Snap",
  "main": "dist/vue-snap.umd.js",
  "module": "dist/vue-snap.es.js",
  "unpkg": "dist/vue-snap.js",
  "engines": {
    "node": ">=20"
  },
  "exports": {
    "import": "./dist/vue-snap.es.js",
    "require": "./dist/vue-snap.umd.js"
  },
  "scripts": {
    "release": "pnpm vite build -c ./config/vite.config.ts",
    "lint": "eslint 'src/**/*.{js,vue}'",
    "test": "test --passWithNoTests",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bartdominiak/vue-snap.git"
  },
  "keywords": [
    "vue",
    "vue-snap",
    "carousel",
    "slider",
    "scroll-snap"
  ],
  "author": "smugers@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bartdominiak/vue-snap/issues"
  },
  "homepage": "https://github.com/bartdominiak/vue-snap#README",
  "peerDependencies": {
    "vue": "^3.5.16"
  },
  "dependencies": {
    "core-js": "^3.43.0",
    "seamless-scroll-polyfill": "^2.3.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lodash": "^4.17.21",
    "sass-embedded": "^1.89.2",
    "vite": "^6.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "browserslist": "> 0.25%, not dead"
}
